;; Shared bits between the syntax extension and the runtime library

(library (
  (name expect_test_common)
  (public_name ppx_expect.common)
  (synopsis "Shared parts for ppx_expect")
  (libraries (base))
  (preprocess (pps (ppx_compare
                    ppx_sexp_conv
                     ppx_driver.runner)))))

(jbuild_version 1)
